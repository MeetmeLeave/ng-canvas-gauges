{
  "name": "ng-canvas-gauges",
  "version": "6.0.0",
  "description": "Angular 6+ component wrapper for the canvas-gauges lib written by @Mikhus",
  "scripts": {
    "build:example": "ng build example",
    "build:lib": "ng build ng-canvas-gauges",
    "prebuild:lib": "npm run clean",
    "postbuild:lib": "cp -f ./LICENSE  ./README.md ./dist/ng-canvas-gauges",
    "build:pack": "cd dist/ng-canvas-gauges && npm pack",
    "build:package": "npm run build:lib && npm run build:pack",
    "postbuild:package": "mkdir -p lib && cp dist/ng-canvas-gauges/ng-canvas-gauges-*.tgz lib",
    "clean": "rm -rf dist/ lib/",
    "start": "ng serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wayneparrott/ng-canvas-gauges.git"
  },
  "keywords": [
    "Angular",
    "Canvas",
    "Gauges",
    "Thermometer",
    "Meter",
    "Speedometer"
  ],
  "author": "vladimir.martynenko.work@gmail.com",
  "contributors": [
    {
      "name": "Wayne Parrott",
      "url": "http://wayneparrott.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wayneparrott/ng-canvas-gauges/issues"
  },
  "homepage": "https://github.com/wayneparrott/ng-canvas-gauges",
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/canvas-gauges": "^2.1.1",
    "canvas-gauges": "^2.1.5",
    "core-js": "^2.5.4",
    "rx-dom-html": "^7.0.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "ng-packagr": "^4.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
